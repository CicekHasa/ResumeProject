
@{
    ViewBag.Title = "AddTechnology";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Teknoloji Ekleme Sayfası</h2>
<form method="post">
    <label>Teknoloji Başlığı</label>
    <input type="text" class="form-control" name="TechnologyTitle"/>
    <br />
    <label>Teknoloji Değeri</label>
    <input type="text" class="form-control" name="TechnologyValue"/>
    <br />
    <button class="btn btn-primary">Verileri Kaydet</button>
</form>

